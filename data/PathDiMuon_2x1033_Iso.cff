### RecoMuon flux ##########################################################
include "HLTrigger/Muon/data/CommonModules_2x1033.cfi"

### HLT Filter flux ##########################################################
module DiMuonIsoL1Filtered = HLTMuonL1Filter {
      InputTag CandTag = hltMuonLevel1Seed

      double MaxEta = 2.5
      double MinPt = 0.
      int32 MinQuality = -1
      int32 MinN = 2
}

module DiMuonIsoL2PreFiltered = HLTMuonPreFilter {
      InputTag CandTag = L2MuonCandidates

      int32 MinN = 2
      double MaxEta = 2.5
      #int32 MinNhits = 4
      int32 MinNhits = 0
      double MaxDr = 9999.
      double MaxDz = 9999.
      double MinPt = 7.
      double NSigmaPt = 3.9
}

module DiMuonIsoL2IsoFiltered = HLTMuonIsoFilter {
      InputTag CandTag = DiMuonIsoL2PreFiltered
      InputTag IsoTag = L2MuonIsolations

      int32 MinN = 1
}

module DiMuonIsoL3PreFiltered = HLTMuonPreFilter {
      InputTag CandTag = L3MuonCandidates

      int32 MinN = 2
      double MaxEta = 2.5
      #int32 MinNhits = 5
      int32 MinNhits = 0
      double MaxDr = 0.02
      double MaxDz = 9999.
      double MinPt = 7.
      double NSigmaPt = 2.2
}

module DiMuonIsoL3IsoFiltered = HLTMuonIsoFilter {
      InputTag CandTag = DiMuonIsoL3PreFiltered
      InputTag IsoTag = L3MuonIsolations

      int32 MinN = 1
}

sequence diMuonIso = {
    #L1 muon
            l1muonreco & DiMuonIsoL1Filtered
    #L2 muon
          & l2muonreco & DiMuonIsoL2PreFiltered
    #L2 muon isolation
          & l2muonisoreco & DiMuonIsoL2IsoFiltered
    #L3 muon
          & l3muonreco & DiMuonIsoL3PreFiltered
    #L3 muon isolation
          & l3muonisoreco & DiMuonIsoL3IsoFiltered
}
